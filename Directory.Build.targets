<Project>
    <ItemGroup Condition="$(IsPackable) == 'true'">
        <None Include="$(MSBuildThisFileDirectory)LICENSE.md; $(MSBuildThisFileDirectory)README.md">
            <Pack>true</Pack>
            <PackagePath></PackagePath>
        </None>
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.CodeAnalysis.FxCopAnalyzers" />
        <PackageReference Include="Microsoft.SourceLink.GitHub" />
        <PackageReference Include="Nerdbank.GitVersioning" />
        <PackageReference Include="StyleCop.Analyzers" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Update="BenchmarkDotNet">
            <Version>0.12.1</Version>
        </PackageReference>
        <PackageReference Update="Microsoft.CodeAnalysis">
            <Version>3.8.0</Version>
        </PackageReference>
        <PackageReference Update="Microsoft.CodeAnalysis.FxCopAnalyzers">
            <Version>3.3.1</Version>
            <PrivateAssets>all</PrivateAssets>
        </PackageReference>
        <PackageReference Update="Microsoft.NET.Test.Sdk">
            <Version>16.8.0</Version>
        </PackageReference>
        <PackageReference Update="Microsoft.SourceLink.GitHub">
            <Version>1.1.0-beta-20204-02</Version>
            <PrivateAssets>all</PrivateAssets>
        </PackageReference>
        <PackageReference Update="Microsoft.TestPlatform.ObjectModel">
            <Version>16.8.0</Version>
        </PackageReference>
        <PackageReference Update="Nerdbank.GitVersioning">
            <Version>3.3.37</Version>
            <PrivateAssets>all</PrivateAssets>
        </PackageReference>
        <PackageReference Update="Nett">
            <Version>0.15.0</Version>
        </PackageReference>
        <PackageReference Update="NuGet.Versioning">
            <Version>5.8.0</Version>
        </PackageReference>
        <PackageReference Update="ReadLine">
            <Version>2.0.1</Version>
        </PackageReference>
        <PackageReference Update="StyleCop.Analyzers">
            <Version>1.2.0-beta.261</Version>
            <PrivateAssets>all</PrivateAssets>
        </PackageReference>
        <PackageReference Update="System.CommandLine">
            <Version>2.0.0-beta1.20371.2</Version>
        </PackageReference>
        <PackageReference Update="xunit">
            <Version>2.4.1</Version>
        </PackageReference>
        <PackageReference Update="xunit.runner.visualstudio">
            <Version>2.4.3</Version>
        </PackageReference>
    </ItemGroup>
</Project>
